{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "Team League",
    "version": "1.0.0",
    "minimum-overwolf-version": "0.97.208.0",
    "author": "ichimatsu",
    "icon": "assets/img/icon.png",
    "icon_gray": "assets/img/icon.png",
    "description": "rocket league team app"
  },
  "permissions": [
    "GameInfo",
    "Profile"
  ],
  "data": {
    "start_window": "index",
    "windows": {
      "index": {
        "file": "index.php",
        "transparent": false,
        "clickthrough": false,
        "resizable": true,
        "show_in_taskbar": true,
        "size": {
          "width": 150,
          "height": 150
        },
        "start_position": {
          "Top": 10,
          "Left": 10
        }
      }
    },
    "game_targeting": {
      "type": "dedicated",
      "game_ids": [
        10798
      ]
    },
    "game_events": [
      10798
    ],
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": [
            10798
          ]
        },
        "start_minimized": false
      }
    ]
  }
}
